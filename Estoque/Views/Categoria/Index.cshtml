
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index - Categorias</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                @foreach (var item in ViewBag.Categorias)
                {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nome</td>
                </tr>
            }
                </tr>
            </tbody>
        </table>
</body>
</html>
