
<table class="table table-hover">
<thead>
    <tr>
        <th>Id</th>
        <th>Nome</th>
    </tr>
</thead>
<tbody>
    <tr>
        @foreach (var item in ViewBag.Categorias)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nome</td>
        </tr>
    }
        </tr>
    </tbody>
</table>
