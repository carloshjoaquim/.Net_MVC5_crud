
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Página Inicial da aplicação</h1>

        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nome</th>
                    <th>Quantidade</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    @foreach (var item in ViewBag.Produtos)
                    {   <tr>
                            <td>@item.Id</td>
                            <td>@item.Nome</td>
                            <td>@item.Quantidade</td>
                        </tr>
                    }
                </tr>
            </tbody>
        </table>
    </div>
</body>
</html>
